.view_swiper {
  height: 100px;
}

.view_parent {
  margin-top: 100rpx;
  height: 200rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.view_son {
  height: 100rpx;
}

.search {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 60rpx;
  background-color: AliceBlue;
  margin-top: 5%;
  border: 1rpx solid AliceBlue;
  border-radius: 5rpx;
}

/* 搜索框提示文字样式 */

.search input {
  font-size: 40rpx;
  padding-left: 50rpx;
  height: 60rpx;
}

/* 搜索框小图标 */

.search image {
  width: 40rpx;
  height: 40rpx;
  padding-right: 20rpx;
}


.clean {
  width: 40rpx;
  height: 40rpx;
  padding-right: 20rpx;
}

.inputtext {
  border-bottom: 1px solid gray;
}

.line {
  margin: 0 auto;
  height: 1px;
  margin-left: 20px;
  margin-right: 20px;
  background-color: #d8d8d8;
}

.item1 {
  color: black;
  font-size: 40rpx;
}

.item2 {
  display: flex;
  flex-direction: column;
  color: black;
  background-color: AliceBlue;
  font-size: 40rpx;
  padding-left: 50rpx;
  margin-top: 50rpx;
}

.item3 {
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: AliceBlue;
}

.item4 {
  color: black;
  background-color: AliceBlue;
  font-size: 40rpx;
  padding-left: 20rpx;
}

.item3 image {
  width: 50rpx;
  height: 40rpx;
  padding-left: 20rpx;
}



.item{
  display: flex;
  flex-direction: column;
  margin-left: 10rpx;
  margin-right: 10rpx;
  margin-bottom: 2rpx;
  background-color: AliceBlue;
}

.desc{
  font-size: 35rpx;
  margin-left: 20rpx;
  line-height: 50rpx;
}

.name{
  font-size: 35rpx;
  color: #999999;
  margin-left: 65rpx;
}


.name2{
  font-size: 35rpx;
  color: #999999;
  margin-left: 35rpx;
}

.hr{
  height: 5rpx;
  background-color: #cccccc;
  margin-top: 10rpx;
  opacity: 0.2
}

.hr2{
  height: 2rpx;
  background-color: #cccccc;
